{
    "name": "combindma/sendinblue-tracker",
    "description": "Sendinblue Tracker SDK For PHP",
    "keywords": [
        "Combindma",
        "laravel",
        "sendinblue-tracker"
    ],
    "homepage": "https://github.com/combindma/sendinblue-tracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "webmaster@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\SendinBlueTracker\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\SendinBlueTracker\\SendinBlueTrackerServiceProvider"
            ],
            "aliases": {
                "SendinBlueTracker": "Combindma\\SendinBlueTracker\\Facades\\SendinBlueTracker"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
